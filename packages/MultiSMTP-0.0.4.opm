<?xml version="1.0" encoding="utf-8" ?>
<otrs_package version="1.1">
    <Name>MultiSMTP</Name>
    <Version>0.0.4</Version>
    <Vendor>Perl-Services.de</Vendor>
    <URL>http://perl-services.de/</URL>
    <License>GNU AFFERO GENERAL PUBLIC LICENSE Version 3, November 2007</License>
    <Description Lang="en">A module that allows OTRS to use several SMTP servers.</Description>
    <Description Lang="de">Ein Modul, mit dem man mehrere SMTP-Server f√ºr OTRS nutzen kann.</Description>
    <Framework>3.0.x</Framework>
    <Framework>3.1.x</Framework>
    <BuildDate>2012-04-07 20:34:24</BuildDate>
    <BuildHost>localhost</BuildHost>
    <Filelist>
        <File Location="Kernel/Config/Files/MultiSMTP.xml" Permission="644" Encode="Base64"></File>
        <File Location="Kernel/Language/de_MultiSMTP.pm" Permission="644" Encode="Base64"></File>
        <File Location="Kernel/Modules/AdminMultiSMTP.pm" Permission="644" Encode="Base64"></File>
        <File Location="Kernel/Output/HTML/Standard/AdminMultiSMTPList.dtl" Permission="644" Encode="Base64"></File>
        <File Location="Kernel/Output/HTML/Standard/AdminMultiSMTPForm.dtl" Permission="644" Encode="Base64"></File>
        <File Location="Kernel/System/MultiSMTP.pm" Permission="644" Encode="Base64"></File>
        <File Location="Kernel/System/Email/MultiSMTP.pm" Permission="644" Encode="Base64"></File>
        <File Location="Kernel/System/Email/MultiSMTP/SMTP.pm" Permission="644" Encode="Base64"></File>
        <File Location="Kernel/System/Email/MultiSMTP/SMTPS.pm" Permission="644" Encode="Base64"></File>
        <File Location="Kernel/System/Email/MultiSMTP/SMTPTLS.pm" Permission="644" Encode="Base64"></File>
        <File Location="doc/en/MultiSMTP.pod" Permission="644" Encode="Base64"></File>
    </Filelist>
    <DatabaseInstall Type="post">
        <TableCreate Type="post" Name="ps_multi_smtp">
            <Column AutoIncrement="true" Name="id" PrimaryKey="true" Required="true" Type="INTEGER"></Column>
            <Column Name="type" Required="true" Size="250" Type="VARCHAR"></Column>
            <Column Name="host" Required="true" Size="250" Type="VARCHAR"></Column>
            <Column Name="user" Required="true" Size="200" Type="VARCHAR"></Column>
            <Column Name="password" Required="true" Size="250" Type="VARCHAR"></Column>
            <Column Name="port" Required="true" Size="250" Type="VARCHAR"></Column>
            <Column Name="encrypted" Required="true" Type="SMALLINT"></Column>
            <Column Name="create_time" Required="true" Type="DATE"></Column>
            <Column Name="create_by" Required="true" Type="INTEGER"></Column>
            <Column Name="valid_id" Required="true" Type="SMALLINT"></Column>
            <Column Name="change_time" Required="true" Type="DATE"></Column>
            <Column Name="change_by" Required="true" Type="INTEGER"></Column>
            <ForeignKey ForeignTable="system_user">
                <Reference Foreign="id" Local="create_by">
                </Reference>
                <Reference Foreign="id" Local="change_by">
                </Reference>
            </ForeignKey>
            <ForeignKey ForeignTable="valid">
                <Reference Foreign="id" Local="valid_id">
                </Reference>
            </ForeignKey>
        </TableCreate>
        <TableCreate Type="post" Name="ps_multi_smtp_address">
            <Column AutoIncrement="true" Name="id" PrimaryKey="true" Required="true" Type="INTEGER"></Column>
            <Column Name="smtp_id" Required="true" Type="INTEGER"></Column>
            <Column Name="address" Required="true" Size="250" Type="VARCHAR"></Column>
        </TableCreate>
    </DatabaseInstall>
    <DatabaseUninstall>
        <TableDrop Type="post" Name="ps_multi_smtp">
        </TableDrop>
        <TableDrop Type="post" Name="ps_multi_smtp_address">
        </TableDrop>
    </DatabaseUninstall>
</otrs_package>